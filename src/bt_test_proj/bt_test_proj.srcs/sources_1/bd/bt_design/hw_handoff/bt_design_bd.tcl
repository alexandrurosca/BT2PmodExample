  connect_bd_net -net rx_1 [get_bd_ports bt_rx] [get_bd_pins bt_uart/rx] [get_bd_pins ila_0/probe0]
  set_property -dict [ list \
HDL_ATTRIBUTE.MARK_DEBUG {true} \
 ] [get_bd_nets rx_1]
  connect_bd_net -net rx_1_1 [get_bd_ports usb_rx] [get_bd_pins axi_uartlite_0/rx] [get_bd_pins ila_0/probe2]
  set_property -dict [ list \
HDL_ATTRIBUTE.MARK_DEBUG {true} \
 ] [get_bd_nets rx_1_1]
  connect_bd_net -net sys_clock_1 [get_bd_ports sys_clock] [get_bd_pins clk_wiz_1/clk_in1]

  # Create address segments
  create_bd_addr_seg -range 0x00010000 -offset 0x40600000 [get_bd_addr_spaces microblaze_0/Data] [get_bd_addr_segs axi_uartlite_0/S_AXI/Reg] SEG_axi_uartlite_0_Reg
  create_bd_addr_seg -range 0x00010000 -offset 0x40610000 [get_bd_addr_spaces microblaze_0/Data] [get_bd_addr_segs bt_uart/S_AXI/Reg] SEG_bt_uart_Reg
